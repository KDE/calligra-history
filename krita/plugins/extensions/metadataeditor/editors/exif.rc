                propertyName = "currentIndex" />
    <EntryEditor editorName = "editSharpness"
                schemaUri = "http://ns.adobe.com/exif/1.0/"
                entryName = "Sharpness"
                editorSignal = "activated(int)"
                propertyName = "currentIndex" />
    <EntryEditor editorName = "editContrast"
                schemaUri = "http://ns.adobe.com/exif/1.0/"
